node {
  stage 'buildIndev'
  openshiftBuild(namespace: 'demo', buildConfig: 'welcome', showBuildLogs: 'true')
  
  stage 'deployIndev'
  openshiftDeploy(namespace: 'demo', deploymentConfig: 'welcome')
  openshiftScale(namespace: 'demo', deploymentConfig: 'welcome',replicaCount: '5')
